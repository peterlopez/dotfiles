# SourceTree
alias sourcetree="open -a /Applications/SourceTree.app"
alias st="sourcetree ."

# Docker
alias dockerapp="open -a /Applications/Docker.app"
alias d="dockerapp"
alias dockerappquit="osascript -e 'quit app \"Docker.app\"'"


